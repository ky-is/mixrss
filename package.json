{
	"name": "mixrss",
	"version": "0.1.0",
	"description": "Collaborative playlists built on RSS.",
	"license": "ISC",
	"repository": "https://github.com/ky-is/mixrss",
	"scripts": {
		"serve": "vue-cli-service serve --open",
		"build": "vue-cli-service build --modern",
		"lint": "vue-cli-service lint"
	},
	"engines": {
		"node": "^10.0.0",
		"npm": "^6.0.0"
	},
	"dependencies": {
		"fetch-jsonp": "^1.1.3",
		"vue": "^2.5.17",
		"vue-youtube": "^1.2.5"
	},
	"devDependencies": {
		"@ky-is/vue-cli-plugin-tabs": "^1.0.0",
		"@vue/cli-plugin-babel": "^3.0.0-rc.10",
		"@vue/cli-plugin-eslint": "^3.0.0-rc.10",
		"@vue/cli-plugin-typescript": "^3.0.0-rc.10",
		"@vue/cli-service": "^3.0.0-rc.10",
		"@vue/eslint-config-typescript": "^3.0.0-rc.10",
		"typescript": "^3.0.0",
		"vue-template-compiler": "^2.5.17",
		"vuex": "^3.0.1"
	},
	"babel": {
		"presets": [
			[
				"@vue/app",
				{
					"jsx": false
				}
			]
		]
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": {
		"production": [
			"> 1%",
			"last 2 versions",
			"not ie <= 8"
		],
		"development": [
			"last 1 version",
			"not dead"
		]
	}
