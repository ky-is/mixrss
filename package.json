{
	"name": "mixrss",
	"version": "0.1.0",
	"scripts": {
		"serve": "vue-cli-service serve --open",
		"build": "vue-cli-service build --modern",
		"lint": "vue-cli-service lint"
	},
	"dependencies": {
		"fetch-jsonp": "^1.1.3",
		"vue": "^2.5.17",
		"vue-youtube": "^1.2.5"
	},
	"devDependencies": {
		"@fullhuman/postcss-purgecss": "^1.0.1",
		"@ky-is/eslint-config": "^1.4.1",
		"@ky-is/tailwind-color-palette": "^0.1.0",
		"@ky-is/tailwindcss-plugin-width-height": "^0.1.0",
		"@ky-is/vue-cli-plugin-tabs": "^1.1.0",
		"@ky-is/vue-cli-plugin-tailwind": "^1.4.0",
		"@vue/cli-plugin-babel": "^3.0.2",
		"@vue/cli-plugin-eslint": "^3.0.2",
		"@vue/cli-plugin-typescript": "^3.0.2",
		"@vue/cli-service": "^3.0.2",
		"@vue/eslint-config-typescript": "^3.0.2",
		"postcss-preset-env": "^5.3.0",
		"tailwindcss": "^0.6.5",
		"typescript": "^3.0.0",
		"vue-template-compiler": "^2.5.17",
		"vuex": "^3.0.1"
	},
	"babel": {
		"presets": [
			[
				"@vue/app",
				{
					"jsx": false
				}
			]
		]
	},
	"eslintConfig": {
		"extends": "@ky-is/eslint-config/vue-typescript",
		"rules": {
			"vue/attributes-order": "error"
		}
	},
	"browserslist": {
		"production": [
			"> 1%",
			"last 2 versions",
			"not ie <= 8"
		],
		"development": [
			"last 1 version",
			"not dead"
		]
	},
	"license": "ISC",
	"repository": "https://github.com/ky-is/mixrss",
	"engines": {
		"node": "^10.0.0",
		"npm": "^6.0.0"
	},
	"description": "Collaborative playlists built on RSS."
}
